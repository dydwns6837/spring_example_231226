<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "../mybatis-3-mapper.dtd">
 
 <mapper namespace="com.example.lesson03.mapper.ReviewMapper">
 	<select id="selectReview" resultType="com.example.lesson03.domain.Review">
 		SELECT
 			`id`
			,`storeId`
			,`menu`
			,`userName`
			,`point`
			,`review`
			,`createdAt`
			,`updatedAt`
		FROM
			`new_review`
		WHERE
			`id` = 5
 	</select>
 </mapper>
